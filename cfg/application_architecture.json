{
    "nodes": [
        {
            "name" : "/sensor_dummy_node",
            "start_node" : true,
            "callbacks": [
                {
                    "type": "timer",
                    "timer_duration_ns": "1000000000",
                    "latency": "./result/node/sensor_dummy_node/timer_callback1.csv",
                    "latency_type": "histogram",
                    "publish": [
                        {
                            "topic_name": "/topic1"
                        }
                    ]
                },
                {
                    "type": "timer",
                    "timer_duration_ns": "1000000000",
                    "latency": "./result/node/sensor_dummy_node/timer_callback2.csv",
                    "latency_type": "timeseries",
                    "publish": [
                        {
                            "topic_name": "/topic2"
                        }
                    ]
                }
            ]
        },
        {
            "name": "/no_dependency_node",
            "callbacks": [
                {
                    "type": "subscribe",
                    "topic_name": "/topic1",
                    "latency": "./result/node/no_dependency_node/subscribe_callback1.csv",
                    "latency_type": "timeseries",
                    "publish": [{
                        "topic_name": "/topic3"
                    }]
                }
            ]
        },
        {
            "name": "/sub_dependency_node",
            "callbacks": [
                {
                    "type": "subscribe",
                    "topic_name": "/topic3",
                    "latency": "./result/node/no_dependency_node/subscribe_callback1.csv",
                    "latency_type": "timeseries",
                    "publish": [
                        {"topic_name": "/topic4"}
                    ]
                },
                {
                    "type": "subscribe",
                    "topic_name": "/topic2",
                    "latency": "./result/node/no_dependency_node/subscribe_callback2.csv",
                    "latency_type": "timeseries",
                    "publish": [
                        {"topic_name": "/topic5"}
                    ]
                }
            ],
            "execution": [
                {
                    "from": "subscribe_topic3",
                    "to": "subscribe_topic2",
                    "latency": "./result/node/sub_dependency_node/subscribe_topic3_to_subscribe_topic2.csv",
                    "latency_type": "timeseries"
                }
            ]
        },
        {
            "name": "/timer_dependency_node",
            "callbacks": [
                {
                    "type": "subscribe",
                    "topic_name": "/topic4",
                    "latency": "./result/node/timer_dependency_node/callback_subscribe1.csv",
                    "latency_type": "timeseries"
                },
                {
                    "type": "timer",
                    "timer_duration_ns": "1000000000",
                    "latency": "./result/node/timer_dependency_node/callback_timer1.csv",
                    "latency_type": "timeseries",
                    "publish": [
                        {"topic_name": "/topic6"}
                    ]
                }
            ],
            "execution": [
                {
                    "from": "subscribe_subscribe1",
                    "to": "timer_1000000000",
                    "latency": "./result/callback/timer_dependency_node/subscribe_subscribe1_to_timer_1000000000.csv",
                    "latency_type": "timeseries"
                }
            ]
        },
        {
            "name": "/actuator_dummy_node",
            "end_node" : true,
            "callbacks": [
                {
                    "type": "subscribe",
                    "topic_name": "/topic5",
                    "latency": "latency/sample_series.csv",
                    "latency_type": "timeseries"
                },
                {
                    "type": "subscribe",
                    "topic_name": "/topic6",
                    "latency": "latency/sample_series.csv",
                    "latency_type": "timeseries"
                }
            ]
        }
    ],
    "communication": [
        {
            "topic_name": "/topic1",
            "publisher": "/sensor_dummy_node",
            "subscriber": "/no_dependency_node",
            "latency": "latency/sample_series.csv",
            "latency_type": "timeseries"
        },
        {
            "topic_name": "/topic2",
            "publisher": "/sensor_dummy_node",
            "subscriber": "/sub_dependency_node",
            "latency": "latency/sample_series.csv",
            "latency_type": "timeseries"
        },
        {
            "topic_name": "/topic3",
            "publisher": "/no_dependency_node",
            "subscriber": "/sub_dependency_node",
            "latency": "latency/sample_series.csv",
            "latency_type": "timeseries"
        },
        {
            "topic_name": "/topic4",
            "publisher": "/sub_dependency_node",
            "subscriber": "/timer_dependency_node",
            "latency": "latency/sample_series.csv",
            "latency_type": "timeseries"
        },
        {
            "topic_name": "/topic5",
            "publisher": "/sub_dependency_node",
            "subscriber": "/actuator_dummy_node",
            "latency": "latency/sample_series.csv",
            "latency_type": "timeseries"
        },
        {
            "topic_name": "/topic6",
            "publisher": "/timer_dependency_node",
            "subscriber": "/actuator_dummy_node",
            "latency": "latency/sample_series.csv",
            "latency_type": "timeseries"
        }
    ]
}
